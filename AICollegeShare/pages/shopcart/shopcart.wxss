page {
    background-color: #f8f8f8;
    padding-bottom: 50px;
}

.shop_check {
    width: 20px;
    height: 100px;
}

.shop_check checkbox {
    position: relative;
    top: 33px;
}

.shopCarItem {
    width: calc(100% - 10px);
    height: 100px;
    margin: 10px auto;
    display: flex;
    flex-wrap: nowrap;
    background-color: #FFFFFF;
}

.shop_img {
    min-width: 100px;
    height: 100px;
}

.shop_img image {
    width: 80px;
    height: 80px;
    position: relative;
    top: 10px;
    left: 10px;
    border-radius: 5px;
}

.shop_text {
    width: calc(100% - 120px);
    height: 80px;
    margin-top: 10px;
    /* background-color: red; */
}

.shop_title {
    width: 100%;
    height: 20px;
    line-height: 20px;
    font-size: 14px;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.shop_numBox {
    width: 100%;
    height: 60px;
    display: flex;
    flex-wrap: nowrap;
}

.shop_price_box {
    width: 100px;
    height: 60px;
}

.shop_price {
    width: auto;
    height: 30px;
    line-height: 30px;
    color: #FF0000;
    position: relative;
    top: 30px;
    font-weight: bold;
}

.shop_num_box {
    width: calc(100% - 100px);
    height: 60px;
}

.shop_num {
    width: 130px;
    height: 25px;
    position: relative;
    top: 10px;
    right: 5px;
    float: right;
    border: 1px solid #00000050;
    display: flex;
    flex-wrap: nowrap;
    justify-content: space-between;
}

.shop_delete {
    width: 100%;
    height: 30px;
    position: relative;
    top: 15px;
    display: flex;
    justify-content: flex-end;
}
.shop_delete image {
    width: 25px;
    height: 25px;
    float: right;
}
.shop_free:active {
    background-color: #e2e2e2;
}

.shop_add:active {
    background-color: #e2e2e2;
}

.shop_free {
    width: 40px;
    height: 100%;
    text-align: center;
    line-height: 25px;
    font-size: 16px;
    font-weight: bold;
    background-color: #FFFFFF;
    border-right: 1px solid #00000050;
}

.shop_add {
    width: 40px;
    height: 100%;
    text-align: center;
    line-height: 25px;
    font-size: 16px;
    font-weight: bold;
    background-color: #FFFFFF;
    border-left: 1px solid #00000050;
}

.shop_count {
    width: auto;
    height: 100%;
    line-height: 25px;
    font-size: 14px;
}




.get_money {
    width: 100%;
    height: 50px;
    border-top: 1px solid #00000050;
    background-color: #FFFFFF;
    display: flex;
    flex-wrap: nowrap;
    justify-content: space-between;
    position: fixed;
    bottom: 0px;
    z-index: 999999;
}

.all_choice {
    width: auto;
    height: 100%;
    line-height: 50px;
    font-size: 14px;
}

.all_choice checkbox {
    position: relative;
    left: 5px;
}


.get_box {
    width: 230px;
    height: 50px;
    display: flex;
    flex-wrap: nowrap;
    justify-content: space-between;
}

.get_box_text {
    width: 130px;
    height: 30px;
    line-height: 30px;
    font-size: 14px;
    display: flex;
    flex-wrap: nowrap;
}

.get_box_text_small {
    font-size: 10px;
}

.get_box_btn {
    width: 80px;
    height: 50px;
    color: #FFFFFF;
    background-color: red;
    line-height: 50px;
    text-align: center;
    font-size: 14px;
}

.get_price {
    font-size: 14px;
    color: #FF0000;
}